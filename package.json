{
    "name": "@m00nbyte/moonbyte.at",
    "version": "1.0.0",
    "description": "Blazing fast static website built with TypeScript, TailwindCSS, and Rollup.",
    "author": "m00nbyte <office@moonbyte.at>",
    "keywords": [
        "moonbyte",
        "portfolio"
    ],
    "license": "MIT",
    "engines": {
        "node": ">=18.12.0"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development rollup -c -w",
        "build": "cross-env NODE_ENV=production rollup -c",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d dist",
        "watch": "npx serve dist",
        "lighthouse:mobile": "npx lighthouse http://localhost:3000/ --quiet --chrome-flags=\"--headless\" --output-path=./stats/lighthouse.mobile.html",
        "lighthouse:desktop": "npx lighthouse http://localhost:3000/ --quiet --preset=desktop --chrome-flags=\"--headless\" --output-path=./stats/lighthouse.desktop.html",
        "lighthouse:all": "yarn run lighthouse:mobile && yarn run lighthouse:desktop",
        "tw-conf": "npx tailwind-config-viewer -o"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "url": "git+ssh://git@github.com/m00nbyte/moonbyte.at.git"
    },
    "homepage": "https://github.com/m00nbyte/moonbyte.at#readme",
    "bugs": {
        "url": "https://github.com/m00nbyte/moonbyte.at/issues"
    },
    "dependencies": {},
